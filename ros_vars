Map.addLayer(liveWoodyBiomass, visParams, 'Aboveground Live Woody Biomass');









var shapes = ee.FeatureCollection("users/dsandovalhdh/fire_perimeter_2011");
var values = daily_prec.map(function(image) {
  return image.reduceRegions({
     collection: shapes,
     reducer: ee.Reducer.mean(),
     scale: 1000
  }).map(function(feature) {
     // Add some identifying information from the image.
     return feature.set('date', image.get('date'))
  })
}).flatten();
// console.log(values);
Export.table.toDrive(values,
"test_radar_small",
"tab_GE",
"test_radar_small");