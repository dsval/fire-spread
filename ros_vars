 var palsar = ee.ImageCollection("JAXA/ALOS/PALSAR/YEARLY/SAR")
 .select('HH','HV')
 .filterDate(start,end)
 .pow(2).log10().multiply(10).subtract(83);


var visParams = {
  min: 0.0,
  max: 350.0,
  palette: [
    'FFFFFF', 'CE7E45', 'DF923D', 'F1B555', 'FCD163', '99B718', '74A901',
